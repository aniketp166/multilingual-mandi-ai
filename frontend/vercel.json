{
  "version": 2,
  "name": "multilingual-mandi",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Multilingual Mandi",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0",
    "NEXT_PUBLIC_ENVIRONMENT": "production",
    "NEXT_PUBLIC_ENABLE_DARK_MODE": "true",
    "NEXT_PUBLIC_ENABLE_OFFLINE_MODE": "true",
    "NEXT_PUBLIC_ENABLE_ANALYTICS": "false",
    "NEXT_PUBLIC_ENABLE_DEBUG_LOGS": "false",
    "NEXT_PUBLIC_STORAGE_VERSION": "1.0.0",
    "NEXT_PUBLIC_MAX_STORAGE_SIZE": "5242880",
    "NEXT_PUBLIC_DEFAULT_LANGUAGE": "en",
    "NEXT_PUBLIC_DEFAULT_CURRENCY": "INR",
    "NEXT_PUBLIC_DEFAULT_LOCATION": "India",
    "NEXT_PUBLIC_THEME": "light",
    "NEXT_PUBLIC_MOBILE_BREAKPOINT": "768",
    "NEXT_PUBLIC_TOUCH_TARGET_SIZE": "44"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}